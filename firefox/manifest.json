{
    "manifest_version": 2,
    "name": "AOL mailto",
    "version": "1.0",
    "description": "Add AOL Mail as a default email provider for all mailto links.",
    "author": "Wesley Branton",

    "developer": {
        "name": "Wesley Branton",
        "url": "http://addons.wesleybranton.com"
    },

    "icons": {
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "64": "icons/icon-64.png",
        "96": "icons/icon-96.png",
        "128": "icons/icon-128.png"
    },

    "browser_specific_settings": {
        "gecko": {
            "id": "aolmail@computerwhiz",
            "strict_min_version": "57.0"
        }
    },

    "protocol_handlers": [{
        "protocol": "mailto",
        "name": "AOL Mail",
        "uriTemplate": "https://mail.aol.com/webmail-std/ComposeMessage?to=%s"
    }]
}
